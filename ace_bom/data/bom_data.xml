<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!--
             Precisions
        -->
        <record forcecreate="True" id="concentration_precision" model="decimal.precision">
            <field name="name">BoM Concentration Precision</field>
            <field name="digits">4</field>
        </record>
        <!--
            sequences
        -->
        <record id="bom_recipe_number_seq" model="ir.sequence">
            <field name="name">BoM Recipe Sequence</field>
            <field name="code">mrp.bom.recipe</field>
            <field name="prefix">RCP</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="implementation">no_gap</field>
        </record>
        <record id="bom_packaging_number_seq" model="ir.sequence">
            <field name="name">BoM Packaging Sequence</field>
            <field name="code">mrp.bom.packaging</field>
            <field name="prefix">PAC</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>
